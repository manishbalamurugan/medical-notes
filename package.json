{
  "name": "real-time-transcription-demo",
  "version": "0.1.0",
  "main": "electron.js",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.521.0",
    "@aws-sdk/client-transcribe-streaming": "^3.523.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.521.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.17",
    "aws-sdk": "^2.1566.0",
    "axios": "^1.6.7",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "ffmetadata": "^1.7.0",
    "ffmpeg-static": "^5.2.0",
    "firebase": "^10.10.0",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "lucide-react": "^0.341.0",
    "microphone-stream": "^6.0.1",
    "multer": "^1.4.5-lts.1",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-audio-player": "^0.17.0",
    "react-audio-voice-recorder": "^2.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.22.1",
    "react-scripts": "5.0.1",
    "react-speech-kit": "^3.0.1",
    "react-timer-hook": "^3.0.7",
    "react-toastify": "^8.0.0",
    "stream": "^0.0.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "electron": "electron .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm:start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "npm run build && electron-builder",
    "electron-win": "npm run build && electron-builder --win"
  },
  "build": {
    "appId": "com.magnumconsulting.medicalnotes",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "electron.js"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ],
      "extraResources": [
        "electron.js"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "extraResources": [
        "electron.js"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3"
  }
}
